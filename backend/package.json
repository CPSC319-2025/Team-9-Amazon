{
  "name": "express-typescript-boilerplate",
  "version": "1.0.14",
  "description": "An Express boilerplate backend",
  "author": "Edwin Hernandez",
  "repository": "edwinhern/express-typescript-2024",
  "license": "MIT",
  "main": "index.ts",
  "private": true,
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/index.ts | pino-pretty",
    "build": "tsup",
    "start": "node dist/index.js",
    "clean": "rimraf dist coverage",
    "lint": "biome check src/",
    "lint:fix": "biome check src/ --fix",
    "format": "biome format src/",
    "test": "vitest run",
    "prepare": "husky",
    "migrate:up": "cd src/database && NODE_OPTIONS=\"-r ts-node/register\" npx sequelize-cli db:migrate",
    "migrate:down": "cd src/database && NODE_OPTIONS=\"-r ts-node/register\" npx sequelize-cli db:migrate:undo:all",
    "seed": "cd src/database && NODE_OPTIONS=\"-r ts-node/register\" npx sequelize-cli db:seed",
    "seed:all": "cd src/database && NODE_OPTIONS=\"-r ts-node/register\" npx sequelize-cli db:seed:all",
    "seed:undo": "cd src/database && NODE_OPTIONS=\"-r ts-node/register\" npx sequelize-cli db:seed:undo",
    "seed:undo:all": "cd src/database && NODE_OPTIONS=\"-r ts-node/register\" npx sequelize-cli db:seed:undo:all"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.0.0",
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/lib-storage": "^3.758.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "express": "^4.19.2",
    "express-jwt": "^8.5.1",
    "express-rate-limit": "^7.2.0",
    "helmet": "^8.0.0",
    "http-status-codes": "^2.3.0",
    "mysql2": "^3.12.0",
    "pino": "^9.4.0",
    "pino-http": "^10.0.0",
    "sequelize": "^6.37.5",
    "swagger-ui-express": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "pino-pretty": "^13.0.0",
    "rimraf": "^6.0.0",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.6",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.4",
    "vite-tsconfig-paths": "^5.0.0",
    "vitest": "^3.0.0"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,json,jsonc}": [
      "biome check --apply --no-errors-on-unmatched"
    ]
  },
  "tsup": {
    "entry": [
      "src",
      "!src/**/__tests__/**",
      "!src/**/*.test.*"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  }
}
